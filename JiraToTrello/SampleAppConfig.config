<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="TrelloCredentials" type="System.Configuration.NameValueSectionHandler" />
    <section name="JiraCredentials" type="System.Configuration.NameValueSectionHandler" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <TrelloCredentials>
    <add key="User" value="TRELLOBOT_USERNAME" />
    <add key="Email" value="TRELLOBOT_EMAIL" />
    <add key="Secret" value="APPSECRET" />
    <add key="AppKey" value="AUTHORIZED_KEY" />
    <add key="PublicKey" value="PUBKEY" />
  </TrelloCredentials>
  
  <JiraCredentials>
    <add key="Site" value="JIRA_WEBSITE" />
    <add key="Project" value="PROJECT" />
    <add key="User" value="JIRABOT_USERNAME" />
    <add key="Password" value="JIRABOT_PASSWORD" />
  </JiraCredentials>

</configuration>